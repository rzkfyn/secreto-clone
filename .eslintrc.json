{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "import/prefer-default-export": 0,
        "no-shadow": 0,
        "arrow-body-style": 0
    }
}
